    "ng2-smart-table": "^1.2.2",
    "ng2-tree": "2.0.0-alpha.5",
    "ngx-bootstrap": "^2.0.5",
    "ngx-echarts": "^2.2.0",
    "ngx-uploader": "2.2.5",
    "normalize.css": "6.0.0",
    "popper.js": "^1.14.3",
    "rxjs": "5.4.0",
    "xlsx": "^0.12.6",
    "yz-ngx-base": "^0.2.13",
    "zone.js": "0.8.11"
  },
  "devDependencies": {
    "@angular/cli": "1.2.8",
    "@angular/compiler-cli": "4.1.3",
    "@types/jasmine": "2.5.38",
    "@types/jquery": "2.0.41",
    "@types/jquery.slimscroll": "1.3.30",
    "@types/lodash": "4.14.61",
    "@types/node": "6.0.69",
    "codelyzer": "3.0.1",
    "gh-pages": "0.12.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "npm-run-all": "4.0.2",
    "protractor": "5.1.0",
    "rimraf": "2.6.1",
    "stylelint": "7.10.1",
    "ts-node": "2.1.2",
    "tslint": "5.2.0",
