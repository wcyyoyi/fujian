    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/platform-server": "4.1.3",
    "@angular/router": "4.1.3",
    "@ngx-translate/core": "6.0.1",
    "@ngx-translate/http-loader": "0.0.3",
    "ag-grid": "^18.1.2",
    "ag-grid-angular": "^18.1.0",
    "animate.css": "3.5.2",
    "bootstrap": "^4.1.1",
    "ckeditor": "4.6.2",
    "core-js": "2.4.1",
    "echarts": "^4.1.0",
    "file-saver": "^1.3.8",
    "font-awesome": "4.7.0",
    "fullcalendar": "3.3.1",
    "ionicons": "2.0.1",
    "jquery": "3.2.1",
    "jquery-slimscroll": "1.3.8",
    "lodash": "4.17.15",
    "moment": "^2.22.2",
    "ng2-ckeditor": "1.1.6",
    "ng2-file-upload": "^1.3.0",
